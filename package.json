{
  "name": "playground",
  "version": "0.0.0",
  "private": true,
  "description": "Cimpress UI playground",
  "author": "Cimpress",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "postinstall": "cd playground && npm install",
    "dev": "cd playground && npm run dev",
    "build": "cd playground && npm run build",
    "preview": "cd playground && npm run preview",
    "lint:check": "eslint .",
    "format:check": "prettier . --ignore-unknown --check",
    "types:check": "tsc -b",
    "clean:output": "rimraf --glob tsconfig*.tsbuildinfo playground/tsconfig*.tsbuildinfo"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
